<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AddItemActivity"
    android:fitsSystemWindows="true"
    android:visibility="visible"
    tools:openDrawer="start">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="60dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="0dp">

                <androidx.cardview.widget.CardView


                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="20dp"
                    android:elevation="20dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="10dp"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="2dp"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/status"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_alignParentLeft="true"
                                android:text="Profile Status: "
                                android:textColor="#000000"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/statusTxt"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_toRightOf="@+id/status"
                                android:hint=""
                                android:textColor="@color/colorPrimary"
                                android:textStyle="bold" />

                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="2dp"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/Suggestion"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_alignParentLeft="true"
                                android:gravity="left"
                                android:text=""
                                android:textStyle="bold" />

                        </RelativeLayout>

                    </LinearLayout>
                </androidx.cardview.widget.CardView>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="100dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">


                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="5dp"
                                android:orientation="vertical">

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="5dp"
                                    android:orientation="horizontal">

                                    <com.google.android.material.textfield.TextInputLayout
                                        android:id="@+id/shippingnameTxt"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentRight="true"
                                        android:layout_marginLeft="5dp"
                                        android:textColorHint="@color/cyanbase"
                                        android:inputType="textPersonName">

                                        <EditText
                                            android:id="@+id/Edittextstoreaddressseller"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_marginLeft="5dp"
                                            android:layout_marginRight="5dp"
                                            android:clickable="false"
                                            android:editable="false"
                                            android:focusable="false"
                                            android:hint="Store Address"
                                            android:singleLine="false"
                                            android:textColor="#000000"
                                            android:textSize="13dp" />

                                    </com.google.android.material.textfield.TextInputLayout>

                                </RelativeLayout>

                                <TextView
                                    android:id="@+id/select_category"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="15dp"
                                    android:text="Select Category"
                                    android:textColor="@color/cyanbase" />

                                <RelativeLayout
                                    android:layout_margin="5dp"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">

                                    <com.smiligenceUAT1.metrozsellerpartner.common.MultiSelectionSpinner
                                        android:id="@+id/categorySpinner"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="15dp"
                                        android:layout_marginTop="10dp" />

                                    <TextView
                                        android:id="@+id/selectedSpinnerData"
                                        android:layout_width="match_parent"
                                        android:layout_marginLeft="15dp"
                                        android:textColor="@color/black"
                                        android:layout_height="wrap_content" />
                                </RelativeLayout>

                            </LinearLayout>
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:orientation="horizontal">

                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginLeft="5dp"
                                android:inputType="textPersonName"
                                android:textColorHint="@color/cyanbase"
                                android:textSize="19dp">

                                <EditText
                                    android:digits="abcdefghijkl mnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ"
                                    android:id="@+id/Edittextfirstnameseller"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="5dp"
                                    android:layout_marginRight="5dp"
                                    android:fontFamily="@font/coustard"
                                    android:hint="First Name"
                                    android:imeOptions="actionSend|flagNoEnterAction"
                                    android:inputType="textPersonName"
                                    android:maxLength="20"
                                    android:textColor="#000000"
                                    android:textSize="13dp"/>

                            </com.google.android.material.textfield.TextInputLayout>


                        </RelativeLayout>


                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:orientation="horizontal">

                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"

                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginLeft="5dp"
                                android:inputType="textPersonName"
                                android:textColorHint="@color/cyanbase"
                                android:textSize="19dp">

                                <EditText
                                    android:id="@+id/Edittextlastnameseller"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="5dp"
                                    android:layout_marginRight="5dp"
                                    android:digits="abcdefghij klmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ"
                                    android:fontFamily="@font/coustard"

                                    android:hint="Last Name"
                                    android:inputType="textPersonName"
                                    android:maxLength="20"
                                    android:textColor="#000000"
                                    android:textSize="13dp" />

                            </com.google.android.material.textfield.TextInputLayout>


                        </RelativeLayout>


                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:orientation="horizontal">


                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"


                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginLeft="5dp"
                                android:inputType="textPersonName"
                                android:textColorHint="@color/cyanbase">

                                <com.xw.repo.XEditText
                                    android:id="@+id/Edittextemailidseller"

                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="5dp"
                                    android:layout_marginRight="5dp"
                                    android:hint="Email id"
                                    android:inputType="textEmailAddress"
                                    android:textColor="#000000"
                                    android:textSize="13dp"
                                    app:x_disableEmoji="true" />

                            </com.google.android.material.textfield.TextInputLayout>


                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:orientation="horizontal">

                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginLeft="5dp"
                                android:inputType="textPersonName"
                                android:textColorHint="@color/cyanbase">

                                <com.xw.repo.XEditText
                                    android:id="@+id/Edittextphonenumberseller"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="5dp"
                                    android:layout_marginRight="5dp"
                                    android:enabled="false"
                                    android:hint="Phone Number"
                                    android:inputType="textPersonName"
                                    android:maxLength="10"
                                    android:textColor="#000000"
                                    android:textSize="13dp"
                                    app:x_disableEmoji="true" />

                            </com.google.android.material.textfield.TextInputLayout>


                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:orientation="horizontal">

                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginLeft="5dp"
                                android:inputType="textPersonName"
                                android:textColorHint="@color/cyanbase">

                                <com.xw.repo.XEditText
                                    android:id="@+id/Edittextstorepincodeseller"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="5dp"
                                    android:layout_marginRight="5dp"
                                    android:clickable="false"
                                    android:editable="false"
                                    android:focusable="false"
                                    android:hint="Pincode"
                                    android:inputType="textPersonName"
                                    android:maxLength="6"
                                    android:textColor="#000000"
                                    android:textSize="13dp"
                                    app:x_disableEmoji="true" />

                            </com.google.android.material.textfield.TextInputLayout>


                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:orientation="horizontal">

                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginLeft="5dp"
                                android:inputType="textPersonName"
                                android:textColorHint="@color/cyanbase">

                                <com.xw.repo.XEditText
                                    android:id="@+id/Edittextbanknameeseller"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="5dp"
                                    android:layout_marginRight="5dp"
                                    android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ "
                                    android:hint="Bank Name"
                                    android:inputType="textPersonName"
                                    android:textColor="#000000"
                                    android:maxLength="20"
                                    android:textSize="13dp"
                                    app:x_disableEmoji="true" />


                            </com.google.android.material.textfield.TextInputLayout>


                        </RelativeLayout>


                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:orientation="horizontal">

                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginLeft="5dp"
                                android:inputType="textPersonName"
                                android:textColorHint="@color/cyanbase">

                                <com.xw.repo.XEditText
                                    android:id="@+id/Edittextbranchnameeseller"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="5dp"
                                    android:layout_marginRight="5dp"
                                    android:digits=" abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ"
                                    android:hint="Branch Name"
                                    android:inputType="textPersonName"
                                    android:textColor="#000000"
                                    android:textSize="13dp"
                                    android:maxLength="20"
                                    app:x_disableEmoji="true" />

                            </com.google.android.material.textfield.TextInputLayout>


                        </RelativeLayout>


                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:orientation="horizontal">

                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginLeft="5dp"
                                android:inputType="textPersonName"
                                android:textColorHint="@color/cyanbase">

                                <EditText

                                    android:id="@+id/Edittextaccountnumberseller"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="5dp"
                                    android:layout_marginRight="5dp"
                                    android:hint="Account Number"
                                    android:inputType="number"
                                    android:maxLength="15"
                                    android:textColor="#000000"
                                    android:textSize="13dp" />

                            </com.google.android.material.textfield.TextInputLayout>


                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:orientation="horizontal">

                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginLeft="5dp"
                                android:inputType="textPersonName"
                                android:textColorHint="@color/cyanbase">

                                <EditText

                                    android:id="@+id/Edittextifsccodeseller"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="5dp"
                                    android:layout_marginRight="5dp"
                                    android:digits="ABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"
                                    android:hint="Ifsc Code"
                                    android:inputType="textCapCharacters"
                                    android:maxLength="11"
                                    android:textColor="#000000"
                                    android:textSize="13dp" />

                            </com.google.android.material.textfield.TextInputLayout>


                        </RelativeLayout>


                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:orientation="horizontal">

                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginLeft="5dp"
                                android:inputType="textPersonName"
                                android:textColorHint="@color/cyanbase">

                                <com.xw.repo.XEditText
                                    android:id="@+id/Edittextbusinessnameseller"

                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="5dp"
                                    android:layout_marginRight="5dp"
                                    android:hint="Store Name"
                                    android:inputType="textPersonName"
                                    android:textColor="#000000"
                                    android:textSize="13dp"
                                    app:x_disableEmoji="true" />

                            </com.google.android.material.textfield.TextInputLayout>


                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:orientation="horizontal">

                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginLeft="5dp"
                                android:inputType="textPersonName"
                                android:textColorHint="@color/cyanbase">

                                <com.xw.repo.XEditText
                                    android:id="@+id/Edittextbusinesstypeseller"

                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="5dp"
                                    android:layout_marginRight="5dp"
                                    android:hint="Business Type"
                                    android:inputType="textPersonName"

                                    android:textColor="#000000"
                                    android:textSize="13dp"
                                    app:x_disableEmoji="true" />

                            </com.google.android.material.textfield.TextInputLayout>


                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:orientation="horizontal">

                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginLeft="5dp"
                                android:inputType="textPersonName"
                                android:textColorHint="@color/cyanbase">

                                <EditText
                                    android:id="@+id/Edittextfssainumberseller"

                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="5dp"
                                    android:layout_marginRight="5dp"
                                    android:digits="1234567890"
                                    android:hint="Fssai Number"
                                    android:inputType="number"
                                    android:maxLength="14"
                                    android:textColor="#000000"
                                    android:textSize="13dp"
                                    app:x_disableEmoji="true" />

                            </com.google.android.material.textfield.TextInputLayout>


                        </RelativeLayout>


                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:orientation="horizontal">

                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginLeft="5dp"
                                android:inputType="textPersonName"
                                android:textColorHint="@color/cyanbase">

                                <EditText
                                    android:id="@+id/Edittextaadharnumberseller"

                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="5dp"
                                    android:layout_marginRight="5dp"
                                    android:digits="1234567890 "
                                    android:hint="Aadhar Number"
                                    android:inputType="number"
                                    android:maxLength="12"
                                    android:textColor="#000000"
                                    android:textSize="13dp"
                                    />

                            </com.google.android.material.textfield.TextInputLayout>


                        </RelativeLayout>


                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:orientation="horizontal">

                            <com.google.android.material.textfield.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginLeft="5dp"
                                android:inputType="textPersonName"
                                android:textColorHint="@color/cyanbase">

                                <EditText
                                    android:id="@+id/Edittextgstseller"

                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="5dp"
                                    android:layout_marginRight="5dp"
                                    android:hint="Gst Number"
                                    android:inputType="textPersonName"
                                    android:maxLength="15"
                                    android:textColor="#000000"
                                    android:textSize="13dp"
                                    />

                            </com.google.android.material.textfield.TextInputLayout>


                        </RelativeLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <RelativeLayout

                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content">

                                <Button
                                    android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                    android:padding="5dp"
                                    android:background="@drawable/b_round"
                                    android:backgroundTint="@color/cyanbase"
                                    android:id="@+id/chhosestoreprofile"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentLeft="true"
                                    android:layout_margin="20dp"
                                    android:textColor="@color/white"
                                    android:gravity="center"
                                    android:text="Store Logo" />


                                <ImageView
                                    android:id="@+id/choosestoreprofileImageview"
                                    android:layout_width="90dp"
                                    android:layout_height="90dp"
                                    android:layout_marginLeft="200dp"
                                    android:layout_marginTop="4dp"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/upload" />

                            </RelativeLayout>
                        </LinearLayout>


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <RelativeLayout

                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content">

                                <Button
                                    android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                    android:padding="5dp"
                                    android:background="@drawable/b_round"
                                    android:backgroundTint="@color/cyanbase"
                                    android:textColor="@color/white"
                                    android:id="@+id/aadhariDProof"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentLeft="true"
                                    android:layout_margin="20dp"
                                    android:gravity="center"
                                    android:text="Aadhar id" />


                                <ImageView
                                    android:id="@+id/aadhariDProofImageview"
                                    android:layout_width="90dp"
                                    android:layout_height="90dp"
                                    android:layout_marginLeft="200dp"
                                    android:layout_marginTop="4dp"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/upload" />

                            </RelativeLayout>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <RelativeLayout

                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content">

                                <Button
                                    android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                    android:padding="5dp"
                                    android:background="@drawable/b_round"
                                    android:backgroundTint="@color/cyanbase"
                                    android:textColor="@color/white"
                                    android:id="@+id/fssainumberButton"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentLeft="true"
                                    android:layout_margin="20dp"
                                    android:gravity="center"
                                    android:text="Fssai Certificate" />


                                <ImageView
                                    android:id="@+id/fssainumberImageview"
                                    android:layout_width="90dp"
                                    android:layout_height="90dp"
                                    android:layout_marginLeft="200dp"
                                    android:layout_marginTop="4dp"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/upload" />
                            </RelativeLayout>
                        </LinearLayout>

                        <Button
                            android:textAppearance="@style/TextAppearance.AppCompat.Small"
                            android:padding="5dp"
                            android:background="@drawable/b_round"
                            android:backgroundTint="@color/cyanbase"
                            android:textColor="@color/white"
                            android:id="@+id/uploaddeliveryboydetails"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="40dp"
                            android:layout_marginBottom="20dp"
                            android:text="Upload KYC"></Button>
                    </LinearLayout>


                </RelativeLayout>
            </RelativeLayout>
        </ScrollView>
    </RelativeLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <include
            layout="@layout/app_bar_main"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

    </FrameLayout>
    <com.google.android.material.navigation.NavigationView
        android:id="@+id/nav_view"
        android:layout_width="wrap_content"
        android:state_checked="true"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:fitsSystemWindows="true"
        android:layout_marginTop="25dp"
        app:headerLayout="@layout/nav_header_main"
        app:menu="@menu/main_drawer" />

</androidx.drawerlayout.widget.DrawerLayout>

